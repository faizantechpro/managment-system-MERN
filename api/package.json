{
  "name": "@identifee/portal-api",
  "private": true,
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "engines": {
    "node": ">=0.12.20 <17"
  },
  "scripts": {
    "build": "npm run build:schemas && npm run build:types && tsc --build && copyfiles \"lib/**/*.*\" -e \"lib/**/*.ts\" -u 1 dist",
    "build:schemas": "ts-node -r tsconfig-paths/register --transpile-only lib/middlewares/openapi/schemasGenerator.ts",
    "build:types": "ts-node -r tsconfig-paths/register --transpile-only lib/middlewares/openapi/typesGenerator.ts",
    "dev": "concurrently \"tsc -w --excludeFiles node_modules,dist,*.tsx,lib/middlewares/openapi/types.gen.ts\" \"npm run dev:openapi\" \"npm run dev:server\"",
    "dev:openapi": "cross-env NODE_ENV=development OPENAPI_TS=true ts-node-dev --files --transpile-only --respawn --exit-child --ignore-watch=dist --ignore-watch=node_modules -r tsconfig-paths/register --watch=./lib/routes/**/*.ts -- lib/middlewares/openapi/typesGenerator.ts",
    "dev:server": "cross-env NODE_ENV=development OPENAPI_TS=true ts-node-dev --files --transpile-only --respawn --exit-child --ignore-watch=dist --ignore-watch=node_modules --inspect --watch=../.env,./lib/* -- lib/start.ts",
    "lint": "eslint config/**/*.js lib/**/*.ts sequelize/**/*.js",
    "lint:fix": "eslint --fix config/**/*.js lib/**/*.ts sequelize/**/*.js",
    "migrate:create": "sequelize-cli migration:generate --name $1",
    "migrate:up": "sequelize-cli db:migrate",
    "migrate:status": "sequelize-cli db:migrate:status",
    "repeatable:create": "sequelize-cli --seeders-path './sequelize/repeatable' seed:generate --name $1",
    "repeatable:all": "sequelize-cli --seeders-path './sequelize/repeatable' db:seed:all",
    "repeatable:file": "sequelize-cli --seeders-path './sequelize/repeatable' db:seed --seed $1",
    "seed:create": "sequelize-cli seed:generate --name $1",
    "seed:all": "sequelize-cli db:seed:all",
    "seed:file": "sequelize-cli db:seed --seed $1",
    "start": "node dist/start.js",
    "test": "node_modules/.bin/jest",
    "test:file": "node_modules/.bin/jest $1"
  },
  "author": "",
  "license": "ISC",
